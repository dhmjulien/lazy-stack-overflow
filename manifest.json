{
  "name": "Lazy Stack Overflow",
  "version": "1.0",
  "description": "Scroll to the accepted answer instantly!",
  "content_scripts": [
    {
      "matches": [
        "http://stackoverflow.com/questions/*",
        "https://stackoverflow.com/questions/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "32": "images/so-32.png",
      "64": "images/so-64.png",
      "128": "images/so-128.png"
    }
  },
  "manifest_version": 2
}