{
  "name": "Lazy Stack Overflow",
  "version": "1.0",
  "description": "Click to get your accepted answer instantly!",
  "permissions": ["declarativeContent", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "158": "images/so-icon.png"
    }
  },
  "icons": {
    "158": "images/so-icon.png"
  },
  "manifest_version": 2
}